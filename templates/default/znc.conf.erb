Version = 1.4

<Listener l>
  Port = <%= @port %>
</Listener>

<% @users.each do |user| -%>
<User <%= user.nick %>>
  <Pass password>
    Method = SHA256
    Salt = <%= user.salt %>
    Hash = <%= user.pass %>
  </Pass>
  <% if user.network -%>

  <Network <%= user.network.name %>>
    Server = <%= user.network.server %> <%= user.network.port %>
  </Network>
  <% end -%>
</User>
<% end -%>
